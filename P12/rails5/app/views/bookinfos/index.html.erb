<p id="notice"><%= notice %></p>

<h1>Bookinfos</h1>

<table>
  <thead>
    <tr>
      <th>Acct</th>
      <th>Title</th>
      <th>Author</th>
      <th>Edition</th>
      <th>Publication</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookinfos.each do |bookinfo| %>
      <tr>
        <td><%= bookinfo.acct %></td>
        <td><%= bookinfo.title %></td>
        <td><%= bookinfo.author %></td>
        <td><%= bookinfo.edition %></td>
        <td><%= bookinfo.publication %></td>
        <td><%= link_to 'Show', bookinfo %></td>
        <td><%= link_to 'Edit', edit_bookinfo_path(bookinfo) %></td>
        <td><%= link_to 'Destroy', bookinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookinfo', new_bookinfo_path %>
